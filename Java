import java.math.BigDecimal;
import java.util.Scanner;

public class SuperCompras {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        
        // Entrada do valor da compra como String para garantir precis√£o
        BigDecimal valor = new BigDecimal(scanner.nextLine());
        
        // Regras de desconto
        BigDecimal cinquenta = new BigDecimal("50.00");
        BigDecimal cem = new BigDecimal("100.00");
        
        if (valor.compareTo(cinquenta) < 0) {
            System.out.println("Desconto de 0%");
        } else if (valor.compareTo(cinquenta) >= 0 && valor.compareTo(cem) <= 0) {
            System.out.println("Desconto de 10%");
        } else {
            System.out.println("Desconto de 20%");
        }
        
        scanner.close();
    }
}
